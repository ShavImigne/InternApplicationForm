<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:id="@+id/ScrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/Linearlayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:scrollbars="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/LinearLayoutMain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/InternApplication"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/InternAppHeading"
                    android:textSize="15pt"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Space
                    android:id="@+id/space"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="611dp"
                    app:layout_constraintBottom_toTopOf="@+id/EditEmail"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/InternApplication" />

                <TextView
                    android:id="@+id/NameOfApplicant"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/ApplicantName"
                    android:textAlignment="center"
                    android:textSize="10pt"
                    app:layout_constraintEnd_toEndOf="@+id/EmailAddress"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/EditFullName" />

                <EditText
                    android:id="@+id/EditFullName"
                    android:layout_width="122dp"
                    android:layout_height="44dp"
                    android:gravity="center_horizontal"
                    android:hint="@string/FeedbackName"
                    android:importantForAutofill="no"
                    android:inputType="textPersonName"
                    app:layout_constraintBottom_toTopOf="@+id/EditEmail"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/EditEmail" />

                <TextView
                    android:id="@+id/EmailAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/EmailAddress"
                    android:textAlignment="center"
                    android:textSize="10pt"
                    app:layout_constraintEnd_toEndOf="@+id/UniversitySelect"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/EditFullName" />

                <EditText
                    android:id="@+id/EditEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:gravity="center_horizontal"
                    android:hint="@string/FeedbackEmail"
                    android:importantForAutofill="no"
                    android:inputType="textEmailAddress"
                    app:layout_constraintBottom_toTopOf="@+id/UniversityDropdown"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/UniversityDropdown" />

                <TextView
                    android:id="@+id/UniversitySelect"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/UniversitySelect"
                    android:textAlignment="center"
                    android:textSize="9pt"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/UniversityDropdown" />

                <Spinner
                    android:id="@+id/UniversityDropdown"
                    android:layout_width="211dp"
                    android:layout_height="28dp"
                    android:entries="@array/UniversityName"
                    android:prompt="@string/UniversityPrompt"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toTopOf="@+id/space2"
                    app:layout_constraintEnd_toEndOf="parent" />

                <Space
                    android:id="@+id/space2"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginTop="194dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/LanguageList"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/LanguageList"
                    android:textAlignment="center"
                    android:textSize="10pt"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/space2" />

                <Button
                    android:id="@+id/NewLanguageButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/AddLanguageButton"
                    android:textAlignment="center"
                    app:layout_constraintBottom_toTopOf="@+id/space4"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <Space
                    android:id="@+id/space4"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    app:layout_constraintBottom_toTopOf="@+id/LLSub5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/LLSub5"
                    android:layout_width="411dp"
                    android:layout_height="211dp"
                    android:layout_marginTop="313dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/LanguageName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/LanguageName"
                        android:textAlignment="center"
                        android:textSize="8pt"
                        app:layout_constraintBottom_toBottomOf="@+id/EnterLanguage"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/EnterLanguage" />

                    <EditText
                        android:id="@+id/EnterLanguage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autofillHints="no"
                        android:ems="10"
                        android:hint="@string/FeedbackLanguageHint"
                        android:inputType="textPersonName"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toTopOf="@+id/EnterExperience"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <TextView
                        android:id="@+id/LangExp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/LanguageExperience"
                        android:textAlignment="center"
                        android:textSize="8pt"
                        app:layout_constraintBottom_toBottomOf="@+id/EnterExperience"
                        app:layout_constraintStart_toStartOf="@+id/LanguageName"
                        app:layout_constraintTop_toBottomOf="@+id/EnterLanguage" />

                    <EditText
                        android:id="@+id/EnterExperience"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autofillHints="no"
                        android:ems="10"
                        android:hint="@string/FeedbackExpHint"
                        android:inputType="number"
                        android:textAlignment="center"
                        app:layout_constraintBottom_toTopOf="@+id/space5"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <Space
                        android:id="@+id/space5"
                        android:layout_width="match_parent"
                        android:layout_height="21dp"
                        app:layout_constraintBottom_toTopOf="@+id/Proficiency2"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <CheckBox
                        android:id="@+id/Proficiency1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/LangProf1"
                        app:layout_constraintBottom_toBottomOf="@+id/Proficiency2"
                        app:layout_constraintStart_toStartOf="parent" />

                    <CheckBox
                        android:id="@+id/Proficiency2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/LangProf2"
                        app:layout_constraintEnd_toStartOf="@+id/Proficiency3"
                        app:layout_constraintHorizontal_bias="0.439"
                        app:layout_constraintStart_toEndOf="@+id/Proficiency1"
                        app:layout_constraintTop_toTopOf="@+id/Proficiency3" />

                    <CheckBox
                        android:id="@+id/Proficiency3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="65dp"
                        android:text="@string/LangProf3"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <Button
                        android:id="@+id/LLSub5.4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/RemoveLanguage"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/Proficiency2"
                        app:layout_constraintVertical_bias="1.0"
                        tools:layout_constraintTop_toBottomOf="@id/LLsub5.3" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>